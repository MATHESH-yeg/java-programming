import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String name = sc.next();
        int a = sc.nextInt();
        int c = sc.nextInt();
        if(a==1 && c>70){
            System.out.println("Name of the Student:"+name);
            System.out.printf("%s ",name+" is Eligible for Placement");
        }
        else if((a==1||a==2) && c>75){
            System.out.println("Name of the Student:"+name);
            System.out.printf("%s ",name+" is Eligible for Placement");
        }
        else{
            System.out.println("Name of the Student:"+name);
            System.out.printf("%s ",name+" is Not Eligible for Placement");
        }
        
    }
}
